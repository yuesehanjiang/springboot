package com.yun.pojo;

import java.io.Serializable;

import com.fasterxml.jackson.annotation.JsonInclude;

@JsonInclude(JsonInclude.Include.NON_EMPTY)
public class FirmManage implements Serializable {

    private static final long serialVersionUID = 1L;

    private Integer id; // 企业管理表id
    private String firmName; // 企业名称
    private String firmAgent; // 企业联系人
    private String phone; // 联系方式
    private String firmAddress; // 企业地址
    private String firmAccount; // 平台账号 手机号
    private Integer status; // 状态 1 启用 2 停用
    private String createTime; // 创建时间
    private String updateTime; // 修改时间

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getFirmName() {
        return firmName;
    }

    public void setFirmName(String firmName) {
        this.firmName = firmName;
    }

    public String getFirmAgent() {
        return firmAgent;
    }

    public void setFirmAgent(String firmAgent) {
        this.firmAgent = firmAgent;
    }

    public String getPhone() {
        return phone;
    }

    public void setPhone(String phone) {
        this.phone = phone;
    }

    public String getFirmAddress() {
        return firmAddress;
    }

    public void setFirmAddress(String firmAddress) {
        this.firmAddress = firmAddress;
    }

    public String getFirmAccount() {
        return firmAccount;
    }

    public void setFirmAccount(String firmAccount) {
        this.firmAccount = firmAccount;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public String getCreateTime() {
        return createTime;
    }

    public void setCreateTime(String createTime) {
        this.createTime = createTime;
    }

    public String getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(String updateTime) {
        this.updateTime = updateTime;
    }

}
