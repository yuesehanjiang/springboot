package com.yun.pojo;

import java.io.Serializable;

import com.fasterxml.jackson.annotation.JsonInclude;

@JsonInclude(JsonInclude.Include.NON_EMPTY)
public class UserExperience implements Serializable {
	private static final long serialVersionUID = 1L;
	private Integer id; // 体验用户id
	private String name; // 姓名
	private String phone; // 手机号
	private String createTime; // 创建时间
	private String gymAddr; // 健身房地址码：16进制方式配置
	private String introducePerson;	// 指导人
	private String code;	// 验证码
	
	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getCreateTime() {
		return createTime;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}

 	public String getGymAddr() {
        return gymAddr;
    }

    public void setGymAddr(String gymAddr) {
        this.gymAddr = gymAddr;
    }

    public String getIntroducePerson() {
		return introducePerson;
	}

	public void setIntroducePerson(String introducePerson) {
		this.introducePerson = introducePerson;
	}

}
