package com.yun.mapper;

import org.apache.ibatis.annotations.Mapper;

import com.yun.pojo.Theme;

@Mapper
public interface ThemeMapper {

	String getTheme(String value);	// 查询主题
	
	int saveTheme(Theme theme);	// 添加主题
	
	String getThemeInfo(String value);	// 查询是否已添加过主题
	
	int updateTheme(Theme theme);	// 修改主题
	
}
