Q,GAAA,KAAAR,EAAA,YAAAG,EAAA,SAAiEG,OAAOtC,QAAAgC,EAAA7D,UAAA6B,QAAAC,KAAA+B,EAAA7D,UAAA8B,QAA2D+B,EAAAsB,KAAAtB,EAAAQ,GAAA,KAAAR,EAAAlE,SAAA,OAAAqE,EAAA,gBAAgEG,OAAOxC,MAAAkC,EAAAtF,WAAAoD,MAAA0D,YAAAxB,EAAAtF,WAAAM,SAAAyG,eAAAzB,EAAAtF,WAAAmD,aAA2G8C,IAAKnC,kBAAAwB,EAAAxB,kBAAAE,kBAAAsB,EAAAtB,qBAAqFsB,EAAAsB,MAAA,IAE3/DI,iBADjB,WAAoC,IAAazB,EAAb7E,KAAa8E,eAA0BC,EAAvC/E,KAAuCgF,MAAAD,IAAAF,EAAwB,OAAAE,EAAA,OAAAA,EAAA,OAA2BE,YAAA,cAA1FjF,KAAkHoF,GAAA,KAAAL,EAAA,KAAsBE,YAAA,iBAA2BF,EAAA,QAAnK/E,KAAmKoF,GAAA,iBCEvM,IAcAmB,EAdA9G,EAAA,OAcA+G,CACA5H,EACA8F,GATA,EAVA,SAAA+B,GACAhH,EAAA,SAaA,kBAEA,MAUAiH,EAAA,QAAAH,EAAA,8BC1BAI,EAAAC,QAAA","file":"static/js/18.05ffdd9c2da868b46cdf.js","sourcesContent":["<template>\n  <div class=\"list\">\n    <div class=\"list-header\">\n      <h3 class=\"list-title\">\n        <router-link class=\"router-link\" to='/user/audit_report'>稽核报告</router-link>\n        <span class=\"icon\"></span>\n        <span class='small'>调整图像</span>\n      </h3>\n    </div>\n    <div class=\"list-header\">\n      <h3 class=\"list-title\">\n        <span\n          @click='changeDateTime(index)'\n          class=\"tab-item\"\n          :class=\"{active: item.active}\"\n          v-for='(item, index) in tabs'\n          :key='index'\n        >{{item.value}}</span>\n      </h3>\n      <div class=\"modal-footer\">\n        <span @click=\"cancel\" class='cancel-button'>取消</span>\n        <span @click=\"confirm\" class='confirm-button'>确定</span>\n      </div>\n    </div>\n    <!-- 图像集 -->\n    <div class=\"pictures-container\">\n      <div class='pictures-title'>{{picturesTitle}}</div>\n      <div v-loading='loading' class=\"pictures\">\n        <div\n          @click='changePictureImg(index)'\n          v-for='(item, index) in listData'\n          :key='index'\n          :class=\"{active: item.active}\"\n          class=\"picture-item\">\n          <!-- <img @click='clickPictureItem(item.id)' class=\"picture-item-img\" :src=\"item.pic_path\" alt=\"\"> -->\n          <img class=\"picture-item-img\" :src=\"item.pic_path\" alt=\"\">\n          <span class=\"picture-item-text\">{{item.photo_time}}</span>\n        </div>\n      </div>\n    </div>\n    <div v-if='!loading && !listData.length' class='no-data-container'>\n      <div>\n        <div class=\"gray-logo\"></div>\n        <p class=\"no-data-tips\">\n          <span>暂无数据</span>\n        </p>\n      </div>\n    </div>\n    <toast\n      v-if='toastStatus'\n      :message='toastData.message'\n      :type='toastData.type'>\n    </toast>\n    <g-pagination\n      v-if=\"listData.length\"\n      @changeCurrentPage='changeCurrentPage'\n      @changeCurrentSize='changeCurrentSize'\n      :total='paganition.total'\n      :page-size='paganition.pageSize'\n      :current-page='paganition.currentPage'>\n    </g-pagination>\n  </div>\n</template>\n\n<script>\nimport {getNomemberImageInterval, adjustmentImage, getAdjustTheImageById} from 'api/image'\nimport GPagination from 'components/Pagination'\nimport Modal from 'components/Modal'\nimport Toast from 'components/Toast'\nexport default {\n  components: {Modal, Toast, GPagination},\n  props: {\n    editAuthUserId: Number\n  },\n  data () {\n    return {\n      paganition: {},\n      loading: true,\n      avatorSrc: require('assets/default-avator.png'),\n      // 请求图像列表的json\n      form: {\n        pageNum: 0,\n        pageSize: 10,\n        datetime1: 1\n      },\n      // 修改图像提交的json\n      adjustImgForm: {\n        user_id: this.$route.params.userId,\n        pic_path: '',\n        image_name: '',\n        flag: this.$route.query.operation === 'add' ? '' : '0',\n        uuid: ''\n      },\n      // 服务器数据\n      listData: [],\n\n      allChoosed: false, // 是否权限全选\n\n      createModalStatus: false, // 是否显示创建用户弹窗\n      deleteModalStatus: false, // 删除提示\n      // toast\n      toastStatus: false,\n      toastData: {},\n      // tabs\n      tabs: [{\n        value: '5分钟',\n        active: true\n      }, {\n        value: '6-10分钟',\n        active: false\n      }, {\n        value: '11-30分钟',\n        active: false\n      }]\n    }\n  },\n  computed: {\n    picturesTitle () {\n      let tabs = this.tabs\n      let picturesTitle = ''\n      tabs.forEach(item => {\n        if (item.active) {\n          picturesTitle = `进场${item.value}内图像`\n        }\n      })\n      return picturesTitle\n    }\n  },\n  watch: {\n    '$route': function () {\n      this.getData(Object.assign({}, this.$route.query))\n    }\n  },\n  mounted () {\n    this.getData()\n    this.getAdjustTheImageById()\n  },\n  methods: {\n    // 获取列表\n    getData (data = null) {\n      this.loading = true\n      let form = {\n        pageNum: this.form.pageNum,\n        pageSize: this.form.pageSize\n      }\n      if (data) {\n        form.pageNum = data.pageNum !== undefined ? data.pageNum : form.pageNum\n        form.pageSize = data.pageSize !== undefined ? data.pageSize : form.pageSize\n      }\n      let tabs = this.tabs\n\n      tabs.forEach((item, i) => {\n        if (item.active) {\n          form[`datetime${i + 1}`] = '1'\n        } else {\n          form[`datetime${i + 1}`] = ''\n        }\n      })\n      getNomemberImageInterval(form).then(res => {\n        if (res.code === '200') {\n          this.listData = this.handleListData(res.data)\n          this.paganition = {\n            pageSize: parseInt(res.pageSize),\n            currentPage: parseInt(res.pageNum) + 1,\n            total: parseInt(res.total)\n          }\n        } else if (res.code === '201') {\n          this.listData = []\n        } else {\n          this.listData = []\n          this.$message({\n            message: res.message,\n            type: 'error'\n          })\n        }\n        this.loading = false\n      }).catch(() => {\n        this.loading = false\n      })\n    },\n    easyQuery (data = {}) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      this.$router.push({path: this.$route.path, query})\n    },\n    changeCurrentPage ({pageNum}) {\n      this.easyQuery({\n        pageNum\n      })\n    },\n    changeCurrentSize ({pageSize}) {\n      this.easyQuery({\n        pageNum: '0',\n        pageSize\n      })\n    },\n    getAdjustTheImageById () {\n      getAdjustTheImageById({id: this.$route.query.id}).then(res => {\n        this.adjustImgForm.uuid = res.uuid\n      })\n    },\n    handleListData (listData) {\n      listData.forEach(item => {\n        item.active = false\n      })\n      return listData\n    },\n    // 修改5分钟，6-10分钟\n    changeDateTime (index) {\n      let tabs = this.tabs\n      if (tabs[index].active) {\n        return\n      }\n      tabs.forEach((item, i) => {\n        if (index === i) {\n          item.active = true\n          let key = `datetime${i}`\n          this.form[key] = 1\n        } else {\n          item.active = false\n        }\n      })\n      this.tabs = tabs\n\n      this.getData()\n    },\n    // 以及修改选中的图片\n    changePictureImg (index) {\n      let listData = this.listData\n      if (listData[index].active) {\n        return\n      }\n      listData.forEach((item, i) => {\n        if (index === i) {\n          item.active = true\n          this.adjustImgForm.pic_path = this.listData[index].pic_path\n          this.adjustImgForm.image_name = this.listData[index].image_name\n        } else {\n          item.active = false\n        }\n      })\n      this.listData = listData\n    },\n    cancel () {\n      this.$router.back()\n    },\n    confirm () {\n      if (!this.adjustImgForm.pic_path) {\n        this.setToastData({\n          message: '请选择您要修改的图像',\n          type: 'fail'\n        })\n        return\n      }\n      if (this.adjustImgForm.flag === '') {\n        delete this.adjustImgForm.flag\n      }\n      adjustmentImage(this.adjustImgForm).then(res => {\n        if (res.code === '200') {\n          this.setToastData({\n            message: res.message,\n            callback: () => {\n              this.cancel()\