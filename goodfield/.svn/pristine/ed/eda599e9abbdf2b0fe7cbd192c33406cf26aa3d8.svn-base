      :type='toastData.type'>\n        </toast>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {updatePicSize, getPicSize} from 'api/image'\nimport Toast from 'components/Toast'\nimport Breadcrumb from './../components/Breadcrumb'\nexport default {\n  components: {Toast, Breadcrumb},\n  data () {\n    return {\n      ruleForm: {\n        picSize: ''\n      },\n      rules: {\n        picSize: [\n          { required: true, message: '请输入图像库储存', trigger: 'blur' }\n        ]\n      },\n      loading: true,\n      // toast\n      toastStatus: false,\n      toastData: {}\n\n    }\n  },\n  mounted () {\n    this.getPicSize()\n  },\n  methods: {\n    getPicSize () {\n      getPicSize({}).then(res => {\n        if (res.code === '200') {\n          let ruleForm = this.ruleForm\n          ruleForm.picSize = res.data.picSize\n          this.ruleForm = ruleForm\n        } else {\n          this.setToastData({\n            message: res.message,\n            type: 'fail'\n          })\n        }\n        this.loading = false\n      }).catch(() => {\n        this.loading = false\n      })\n    },\n    confirm () {\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          this.updatePicSize()\n        }\n      })\n    },\n    updatePicSize (picSize = '') {\n      let form = {\n        picSize: picSize === '' ? this.ruleForm.picSize : picSize\n      }\n      updatePicSize(form).then(res => {\n        if (res.code === '200') {\n          this.setToastData({\n            message: res.message\n          })\n          this.getPicSize()\n        } else {\n          this.setToastData({\n            message: res.message,\n            type: 'fail'\n          })\n        }\n      })\n    },\n    setToastData ({message, type, callback}) {\n      this.toastStatus = true\n      this.$set(this.toastData, 'message', message)\n      this.$set(this.toastData, 'type', type)\n      setTimeout(() => {\n        this.toastStatus = false\n        callback && callback()\n      }, 1200)\n    }\n  }\n}\n</script>\n\n<style lang=\"stylus\" scoped>\n    .form\n        margin-left 20px\n    .form-title\n        font-weight normal\n        padding-left 16px\n        margin 30px auto\n        height 23px\n        line-height 23px\n        border-left 4px solid #217EE6\n        font-size 16px\n        color #666666\n    .input-tips\n        padding-left 30px\n        color #666666\n        background url('~@/assets/help.png') no-repeat left\n        background-size 20px 20px\n    .form-footer\n        display flex\n        margin-left 140px\n        padding 20px 0\n        text-align center\n        border none\n        font-size 14px\n    .cancel-button,.confirm-button\n        display inline-block\n        width 110px\n        height 35px\n        line-height 35px\n        text-align center\n        border-radius 35px\n        cursor pointer\n    .cancel-button\n        margin-right 30px\n        color #666666\n        background-color #eaeaea\n        box-shadow 1px 1px 5px #eaeaea\n    .confirm-button\n        color #ffffff\n        background-color rgba(36,207,95,1)\n        box-shadow 1px 1px 5px rgba(36,207,95,1)\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/views/admin/setting/ImageSize.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('breadcrumb'),_vm._v(\" \"),_c('div',{directives:[{name:\"loading\",rawName:\"v-loading\",value:(_vm.loading),expression:\"loading\"}],staticClass:\"form-container\"},[_c('h4',{staticClass:\"form-title\"},[_vm._v(\"图片存量\")]),_vm._v(\" \"),_c('el-form',{ref:\"ruleForm\",staticClass:\"form\",attrs:{\"model\":_vm.ruleForm,\"rules\":_vm.rules,\"label-position\":\"left\",\"label-width\":\"120px\"}},[_c('el-form-item',{staticStyle:{\"width\":\"500px\"},attrs:{\"label\":\"图像库储存(张):\",\"prop\":\"picSize\"}},[_c('el-input',{model:{value:(_vm.ruleForm.picSize),callback:function ($$v) {_vm.$set(_vm.ruleForm, \"picSize\", $$v)},expression:\"ruleForm.pic