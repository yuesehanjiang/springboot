BAA8CN,EAAA,QAAAH,EAAAU,GAAAV,EAAAyC,GAAAF,EAAAC,IAAAI,oBAAyD5C,EAAAU,GAAA,KAAAP,EAAA,mBAAoCG,OAAO2B,KAAA,YAAAC,MAAA,QAAkCC,YAAAnC,EAAAoC,KAAsBC,IAAA,UAAAC,GAAA,SAAAC,GAAiC,WAAAxD,OAAAiB,EAAAnE,OAAAC,MAAAQ,WAAA6D,EAAA,eAAmEG,OAAOuC,IAAMrE,KAAA,uBAAA+D,EAAAC,IAAAE,OAAA5G,MAAAkE,EAAAnE,OAAAC,OAAoEgH,eAAA,gBAA8B9C,EAAAU,GAAA,mBAAAV,EAAAyC,GAAAF,EAAAC,IAAAO,WAAA,oBAAA5C,EAAA,eAA8FG,OAAOuC,IAAMrE,KAAA,uBAAA+D,EAAAC,IAAA5F,GAAAd,MAAAkE,EAAAnE,OAAAC,OAAgEgH,eAAA,gBAA8B9C,EAAAU,GAAA,mBAAAV,EAAAyC,GAAAF,EAAAC,IAAAO,WAAA,4BAAiF,OAAA/C,EAAAU,GAAA,KAAAV,EAAA7D,SAAAT,MAAAsE,EAAA7D,SAAAT,KAAAiD,OAAA,EAAAwB,EAAA,OAAoFE,YAAA,uBAAiCF,EAAA,QAAaG,OAAO0C,MAAAjE,OAAAiB,EAAA7D,SAAA6G,OAAAC,QAAAlE,OAAAiB,EAAA7D,SAAAmB,SAAA,EAAA4F,YAAAnE,OAAAiB,EAAA7D,SAAAoD,UAAA4D,aAAA,GAAAC,gBAAA,GAAAC,aAAA,GAAAjC,UAAA,GAA0LC,IAAKC,YAAAtB,EAAAJ,cAAA0D,sBAAAtD,EAAAH,mBAAwE,GAAAG,EAAAyB,UAE/yH8B,oBCCjB,IAcAC,EAdAhD,EAAA,OAcAiD,CACAC,EACA5D,GATA,EAVA,SAAA6D,GACAnD,EAAA,SAaA,kBAEA,MAUAoD,EAAA,QAAAJ,EAAA","file":"static/js/30.b95183ddddcf6cfee56c.js","sourcesContent":["<template>\n  <div class=\"screen-container\">\n    <!-- <div class=\"screen-empty\" v-if=\"isEmpty\">\n      <img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">\n      <p class=\"empty-title\">没有更多数据了</p>\n    </div> -->\n    <div class=\"screen-empty\" v-if=\"isNoPower\">\n      <img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">\n      <p class=\"empty-title\">没有模块权限，请与管理员确认</p>\n    </div>\n    <div class=\"screen-body\" v-else v-loading=\"loading\">\n      <div class=\"screen-body-header\">\n        <Select size=\"default\" :clearable=\"true\" placeholder=\"会员\" :transfer=\"false\" class=\"query-select\"\n                style=\"width:200px\"\n                :value=\"$route.query.user_type\" @on-change=\"(value)=>{user_type=value}\">\n          <Option value=\"0\">曾会员</Option>\n          <Option value=\"1\">会员</Option>\n          <Option value=\"2\">非会员</Option>\n        </Select>\n        <Input size=\"default\" prefix=\"ios-search\" placeholder=\"姓名/ID/卡号搜索\" style=\"width: 200px\"\n               :value=\"$route.query.keyword\" @on-change=\"handleSearch\"/>\n        <a v-if=\"listData.data&&listData.data.length\" class=\"export-table\" :href=\"exportHref\">导出表格</a>\n        <div class=\"button-group\">\n          <div class=\"order-btn\" :class=\"{'active-order':order==='positive'}\" @click=\"positive\">\n            <span>正序</span>\n            <i class=\"order-icon\"></i>\n          </div>\n          <div class=\"order-btn\" :class=\"{'active-order':order==='reverse'}\" @click=\"reverse\">\n            <span>倒序</span>\n            <i class=\"order-icon order-reverse\"></i>\n          </div>\n        </div>\n      </div>\n      <div class=\"screen-body-container\">\n        <el-table\n          class=\"list\"\n          :header-cell-style=\"{'text-align': 'center'}\"\n          style=\"text-align: center\"\n          height=\"100%\"\n          :data=\"listData.data\"\n          border>\n          <el-table-column\n            prop=\"index\"\n            width=\"70\"\n            label=\"序号\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_name\"\n            label=\"会员姓名\">\n          </el-table-column>\n          <el-table-column\n            prop=\"stores_name\"\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"userId\"\n            label=\"会员ID\">\n            <template slot-scope='scope'>\n              <div>\n                <span v-if='scope.row.userId'>{{scope.row.userId}}</span>\n                <span v-else>-/-</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"user_card\"\n            label=\"卡号\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_pic\"\n            label=\"头像\">\n            <template slot-scope=\"scope\">\n              <table-image v-if=\"$route.query.user_type !== '2'\" :table-image-url='scope.row.user_pic'>\n                <img class=\"user-avatar\" :src=\"scope.row.user_pic\" alt=\"user-avatar\">\n              </table-image>\n              <span v-else>{{scope.row.user_pic}}</span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"exclusive\"\n            label=\"入场次数\">\n            <template slot-scope=\"scope\">\n              <router-link\n                v-if=\"Number($route.query.user_type)!==2\"\n                :to=\"{path:'/user/report/member/'+scope.row.userId,query:$route.query}\"\n                active-class=\"active-tab\">\n                {{scope.row.frequency}}\n              </router-link>\n              <router-link\n                v-else\n                :to=\"{path:'/user/report/member/'+scope.row.id,query:$route.query}\"\n                active-class=\"active-tab\">\n                {{scope.row.frequency}}\n              </router-link>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <div v-if=\"listData.data && listData.data.length > 0\" class=\"screen-body-footer\">\n        <Page\n          :total=\"Number(listData.total)\"\n          :current=\"Number(listData.pageNum)+1\"\n          :page-size=\"Number(listData.pageSize)\"\n          show-sizer show-elevator show-total :transfer=\"false\"\n          @on-change=\"pageNumChange\"\n          @on-page-size-change=\"pageSizeChange\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { getToken } from 'utils/tool'\nimport { getMemberReport } from 'api/report'\nimport TableImage from '../components/TableImage'\nimport Utils from 'utils/utils'\n\nexport default {\n  components: {TableImage},\n  data () {\n    let order = 'positive'\n    let {query} = this.$route\n    if (query.reverse === '1') {\n      order = 'reverse'\n    }\n    return {\n      loading: true,\n      isEmpty: false,\n      isNoPower: false,\n      listData: {},\n      stores: [],\n      stores_id: '',\n      user_type: '',\n      keyword: '',\n      order: order,\n      token: {}\n    }\n  },\n  computed: {\n    exportHref () {\n      let data = {\n        user_id: this.token.id,\n        user_type: '1',\n        keyword: ''\n      }\n      let query = Object.assign({}, data, this.$route.query)\n      delete query['pageNum']\n      delete query['pageSize']\n      return '/goodfield/web/entrancereport/export?' + Object.keys(query).map(v => v + '=' + (query[v] || '')).join('&')\n    }\n  },\n  watch: {\n    stores_id: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        stores_id: value === undefined ? '' : String(value)\n      })\n    },\n    user_type: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        user_type: value === undefined ? '1' : String(value)\n      })\n    },\n    keyword: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        keyword: value || ''\n      })\n    },\n    '$route': function (value) {\n      let {query} = value\n      this.getList(Object.assign({}, query))\n      if (query.positive) {\n        this.order = 'positive'\n      } else if (query.reverse) {\n        this.order = 'reverse'\n      }\n    }\n  },\n  mounted () {\n    this.token = getToken()\n    this.getList(Object.assign({}, this.$route.query))\n  },\n  methods: {\n    handleSearch: Utils.throttle(function (e) {\n      this.keyword = e.target.value\n    }, 1000, {leading: false}),\n    positive () {\n      this.easyQuery({\n        positive: '1',\n        reverse: ''\n      })\n    },\n    reverse () {\n      this.easyQuery({\n        positive: '',\n        reverse: '1'\n      })\n    },\n    easyQuery (data) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      this.$router.push({path: this.$route.path, query})\n    },\n    getList (data = {}) {\n      this.loading = true\n      data['user_id'] = this.token.id\n      data['pageNum'] = data['pageNum'] || 0\n      data['pageSize'] = data['pageSize'] || 10.0\n      data['user_type'] = data['user_type'] || 1\n      getMemberReport(data).then((res) => {\n        if (Number(res.code) === 200) {\n          this.isEmpty = false\n          this.listData = this.countIndex(res)\n          this.stores = res.roles\n        } else if (Number(res.code) === 201) {\n          this.isEmpty = true\n          this.listData = {}\n          this.stores = res.data\n        } else if (Number(res.code) === 202) {\n          this.isEmpty = true\n          this.listData = {}\n          this.isNoPower = true\n        } else {\n          this.listData = {}\n          this.isEmpty = true\n          this.$message({\n            message: res.message,\n            type: 'warning'\n          })\n        }\n        this.loading = false\n      }).catch(() => {\n        this.isEmpty = true\n        this.listData = {}\n        this.loading = false\n      })\n    },\n    countIndex (res) {\n      let {pageSize, pageNum, data} = res\n      if (data instanceof Array) {\n        data.forEach((v, i) => {\n          v.index = (pageSize * pageNum) + i + 1\n        })\n      } else {\n        data.index = 1\n        data = [data]\n      }\n      return res\n    },\n    pageNumChange (pageNum) {\n      this.easyQuery({\n        pageNum: String(pageNum - 1)\n      })\n    },\n    pageSizeChange (pageSize) {\n      this.easyQuery({\n        pageNum: String(0),\n        pageSize: pageSize\n      })\n    }\n  }\n}\n</script>\n<style lang='stylus' scoped>\n  .screen-container\n    height 100%\n    width 100%\n    display flex\n    flex-direction column\n\n  .screen-empty\n    flex 1\n    display flex\n    justify-content center\n    align-items center\n    flex-direction column\n\n  .empty-title\n    font-size 12px\n    color #b1b1b1\n    margin-top 10px\n    > a\n      color #24CF5F\n\n  .screen-body\n    flex 1\n    display flex\n    flex-direction column\n    align-items