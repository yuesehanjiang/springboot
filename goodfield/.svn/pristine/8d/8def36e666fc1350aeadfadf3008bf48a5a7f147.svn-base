v class=\"pic-list-container\" v-loading=\"loading\" ref=\"box\" @scroll=\"handleScroll\">\n      <div class=\"hidden-scroll\">\n        <ul class=\"pic-list\">\n          <li>\n            <img class=\"avatar-img\" :src=\"avatar\" alt=\"\">\n            <span class=\"avatar-title\">安科登记头像</span>\n          </li>\n        </ul>\n      </div>\n      <div class=\"screen-empty\" v-if=\"isEmpty\">\n        <!--<img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">-->\n        <p class=\"empty-title\">暂无数据</p>\n      </div>\n      <div class=\"hidden-scroll\" v-else>\n        <ul class=\"pic-list\">\n          <li v-for=\"(item) in picList\" :key=\"item.id\">\n            <img class=\"avatar-img\" :src=\"item.pic_path\" alt=\"\">\n            <p class=\"avatar-title text-ellipsis\">{{item.photo_time}}</p>\n          </li>\n        </ul>\n      </div>\n      <p class=\"list-empty-tips\" v-if=\"isNoMore&&!isEmpty\">没有更多了</p>\n    </div>\n  </div>\n</template>\n<script>\nimport Breadcrumb from 'views/user/components/Breadcrumb'\nimport { getToken } from 'utils/tool'\nimport { getMemberPicList } from 'api/member'\nimport Utils from 'utils/utils'\nimport Viewer from 'viewerjs'\nimport 'viewerjs/dist/viewer.min.css'\n\nexport default {\n  components: {Breadcrumb},\n  data () {\n    return {\n      avatar: '',\n      loading: true,\n      isEmpty: false,\n      isNoMore: false,\n      listData: {},\n      dateRange: [],\n      picList: []\n    }\n  },\n  watch: {\n    '$route': function () {\n      this.picList = []\n      this.isNoMore = false\n      this.isEmpty = false\n      this.getList()\n    },\n    dateRange: function (value) {\n      if (value[0] === value[1]) {\n        this.$router.replace({\n          path: this.$route.path,\n          query: {\n            datetime: value[0]\n          }\n        })\n      } else {\n        this.$router.replace({\n          path: this.$route.path,\n          query: {\n            startTime: value[0],\n            endTime: value[