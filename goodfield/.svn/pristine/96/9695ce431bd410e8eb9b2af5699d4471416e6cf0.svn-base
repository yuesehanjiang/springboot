   </div>\n          <div>\n            <span class='outline-icon'></span>\n            离线\n          </div>\n          <div @click='refreshData' class='refresh-icon'>\n            刷新\n          </div>\n        </div>\n      </div>\n      <div class='terminal-table-container'>\n        <!-- 已分发 -->\n        <el-table\n          v-show='tabIndex === 0'\n          v-loading=\"loading\"\n          :span-method=\"objectSpanMethod\"\n          :data=\"listData\"\n          border\n          style=\"width: 100%;\">\n          <el-table-column\n            prop=\"index\"\n            label=\"序号\"\n            align='center'\n            width=\"60\">\n          </el-table-column>\n          <el-table-column\n            prop=\"username\"\n            align='center'\n            label=\"用户姓名\">\n          </el-table-column>\n          <el-table-column\n            ref='account'\n            prop=\"account\"\n            align='center'\n            label=\"登录账号\">\n          </el-table-column>\n          <el-table-column\n            ref='phone'\n            prop=\"phone\"\n            align='center'\n            label=\"绑定手机号码\">\n          </el-table-column>\n          <el-table-column\n            prop=\"storeName\"\n            align='center'\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"machineName\"\n            align='center'\n            label=\"终端机\">\n          </el-table-column>\n          <el-table-column\n            prop=\"machineStatus\"\n            align='center'\n            label=\"状态\">\n            <template slot-scope='scope'>\n              <span class=\"status-radius online\" :class=\"{outline: !scope.row.machineStatus}\"></span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"freeStorage\"\n            align='center'\n            label=\"内存余量（GB）\">\n            <template slot-scope='scope'>\n              <span>\n                <span :class='{red: Number(scope.row.freeStorage.top) < 0.48}'>上屏:{{scope.row.freeStorage.top}}；</span>\n                <span :class='{red: Number(scope.row.freeStorage.cenBottom) < 0.48}'>中下屏:{{scope.row.freeStorage.cenBottom}}</span>\n              </span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"machineNumber\"\n            align='center'\n            label=\"数量\">\n          </el-table-column>\n          <el-table-column\n            prop=\"machineItemCount\"\n            align='center'\n            :label=\"machineTotalLabel\">\n          </el-table-column>\n        </el-table>\n        <!-- 未分发 -->\n        <el-table\n          v-show='tabIndex === 1'\n          v-loading=\"loading\"\n          :data=\"newListData\"\n          empty-text='终端机已分发完成'\n          :span-method=\"objectSpanMethod1\"\n          border\n          style=\"width: 100%;\">\n          <el-table-column\n            prop=\"index\"\n            label=\"序号\"\n            align='center'\n            width=\"60\">\n          </el-table-column>\n          <el-table-column\n            prop=\"cityName\"\n            align='center'\n            label=\"地区\">\n          </el-table-column>\n          <el-table-column\n            prop=\"storeName\"\n            align='center'\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"machineName\"\n            align='center'\n            label=\"终端机\">\n          </el-table-column>\n          <el-table-column\n            prop=\"machineStatus\"\n            align='center'\n            label=\"状态\">\n            <template slot-scope='scope'>\n              <span class=\"status-radius online\" :class=\"{outline: !scope.row.machineStatus}\"></span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"freeStorage\"\n            align='center'\n            label=\"内存余量（GB）\">\n            <template slot-scope='scope'>\n              <span>\n                <span :class='{red: Number(scope.row.freeStorage.top) < 0.48}'>上屏:{{scope.row.freeStorage.top}}；</span>\n                <span :class='{red: Number(scope.row.freeStorage.cenBottom) < 0.48}'>中下屏:{{scope.row.freeStorage.cenBottom}}</span>\n              </span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"frequency\"\n            align='center'\n            label=\"数量\">\n          </el-table-column>\n          <el-table-column\n            prop=\"total\"\n            align='center'\n            :label=\"machineTotalLabel\">\n          </el-table-column>\n        </el-table>\n      </div>\n      <g-pagination\n        v-if='listData.length || newListData.length'\n        @changeCurrentPage='changeCurrentPage'\n        @changeCurrentSize='changeCurrentSize'\n        :total='paganition.total'\n        :page-size='paganition.pageSize'\n        :current-page='paganition.currentPage'>\n      </g-pagination>\n      <toast\n        v-if='toastStatus'\n        :message='toastData.message'\n        :type='toastData.type'>\n      </toast>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getAdminMachine, getAdminMachineDistribute, getMemorys} from 'api/terminal'\nimport Utils from 'utils/utils'\nimport Modal from 'components/Modal'\nimport Toast from 'components/Toast'\nimport GPagination from 'components/Pagination'\nimport Breadcrumb from './../components/Breadcrumb'\nexport default {\n  components: {Modal, Toast, GPagination, Breadcrumb},\n  data () {\n    let {status, keyword} = this.$route.query\n    return {\n      tabIndex: 0,\n      tabs: [{\n        title: '已分发'\n      }, {\n        title: '未分发'\n      }],\n      // 服务器数据\n      status,\n      originData: null, // 原始数据\n      keyword,\n      memory: [], // 剩余内存\n      listData: [], // 已分发的数据\n      newListData: [], // 未分发的数据\n      loading: true, // 是否正在加载数据\n      machineTotalLabel: '总数量',\n      paganition: {},\n      // toast\n      toastStatus: false,\n      toastData: {},\n      // 【列表状态筛选】\n      statusOptions: [{\n        status: '',\n        label: '全部'\n      }, {\n        status: '1',\n        label: '在线'\n      }, {\n        status: '0',\n        label: '离线'\n      }]\n    }\n  },\n  computed: {\n    searchPlaceholder () {\n      if (!this.tabIndex) {\n        return '可搜索用户名、登录账号、绑定手机'\n      } else {\n        return '可搜索门店'\n      }\n    }\n  },\n  watch: {\n    '$route': function () {\n      let {pageNum, pageSize, status} = this.$route.query\n      if (pageNum === undefined || pageSize === undefined || status === undefined) {\n        this.easyQuery({\n          pageNum: 0,\n          pageSize: 10,\n          status: ''\n        })\n      } else {\n        this.getListData(this.$route.query)\n      }\n    },\n    'tabIndex': function (oV, nV) {\n      let { pageNum, keyword } = this.$route.query\n      if (Number(pageNum) === 0 && keyword === '') {\n        this.getListData(this.$route.query)\n      } else {\n        this.easyQuery({\n          pageNum: 0,\n          keyword: ''\n        })\n      }\n    },\n    'status': function (ov, nv) {\n      this.easyQuery({\n        pageNum: 0,\n        status: this.status\n      })\n    }\n  },\n  beforeCreate () {\n    let {pageNum, pageSize, status} = this.$route.query\n    if (Object.is(pageNum, undefined) && Object.is(pageSize, undefined) && Object.is(status, undefined)) {\n      this.$router.replace({\n        path: this.$route.path,\n        query: {\n          pageNum: 0,\n          pageSize: 10,\n          status: '' // 默认全部, 1、（''全部） 2、（1在线） 3、（0离线）\n        }\n      })\n    }\n  },\n  mounted () {\n    this.getMemorys()\n    this.getListData(this.$route.query)\n  },\n  methods: {\n    easyQuery (data = {}) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      this.$router.push({path: this.$route.path, query})\n    },\n    changeCurrentPage ({pageNum}) {\n      this.easyQuery({\n        pageNum\n      })\n    },\n    changeCurrentSize ({pageSize}) {\n      this.easyQuery({\n        pageNum: '0',\n        pageSize\n      })\n    },\n    refreshData () {\n      this.getMemorys()\n      this.getListData(this.$route.query)\n    },\n    inputContent: Utils.throttle(function () {\n      this.easyQuery({\n        keyword: this.keyword,\n        pageNum: 0\n      })\n    }, 1000, {leading: false}),\n    changeTabsItem (index) {\n      // this.getMemorys()\n      this.tabIndex = Number(index)\n      this.keyword = ''\n    },\n    // 获取列表\n    getListData (data) {\n      this.loading = true\n      let type = ''\n      let handleListData = ''\n      if (this.tabIndex === 0) {\n        type = getAdminMachine\n        handleListData = this.handleListData\n      } else if (this.tabIndex === 1) {\n        type = getAdminMachineDistribute\n        handleListData = this.handleListData1\n      }\n      let form = Object.assign({}, data)\n      type(form).then(res => {\n        if (res.code === '200') {\n          let listData = this.sortListData(res.data)\n          let machineList = this.buildMachineList(listData)\n          handleListData(listData, machineList)\n          this.machineTotalLabel = `总数量`\n          this.paganition = {\n            pageSize: parseInt(res.pageSize),\n            currentPage: parseInt(res.pageNum) + 1,\n            total: parseInt(res.total)\n          }\n        } else if (res.code === '201') {\n          this.listData = []\n          this.newListData = []\n          this.machineTotalLabel = `总数量`\n        } else {\n          this.listData = []\n          this.machineTotalLabel = `总数量`\n          this.newListData = []\n          this.setToastData({\n            message: res.message,\n            type: 'fail'\n          })\n        }\n        this.loading = false\n      }).catch(() => {\n        this.loading = false\n      })\n    },\n    getMemorys () {\n      getMemorys().then(res => {\n        this.memory = res.data\n      }).catch(() => {\n        this.loading = false\n      })\n    },\n    sortListData (listData) {\n      listData.sort((item1, item2) => {\n        if (item1.id > item2.id) {\n          return 1\n        } else if (item1.id < item2.id) {\n          return -1\n        } else {\n          return 0\n        }\n      })\n      return listData\n    },\n    buildMachineList (listData) {\n      let machineList = []\n      listData.forEach(item1 => {\n        let machineTotal = 0\n        item1.roles.forEach(item2 => {\n          machineTotal += Number(item2.frequency)\n        })\n        machineList.push(machineTotal)\n      })\n      return machineList\n    },\n    /*\n     * 获取后台的数据结构后，构建成一下数据结构\n     * [{\n     *  id: 1,\n     *  username: '用户名',\n     *  account: '登陆账号',\n     *  phone: '绑定手机号',\n     *  storeName: '门店',\n     *  storeId: '门店id',\n     *  machines: [{machine_id: 1, machine_name: '终端机名称1'}, {machine_id: 2, machine_name: '终端机名称2'}],\n     *  machineNumber: ''\n     * }]\n     */\n    // 处理已分发，列表数据\n    handleListData (listData, machineList) {\n      let listDataLength = listData.length\n      let newListData = []\n\n      // 遍历listData\n      for (let i = 0; i < listDataLength; i++) {\n        let listItem = listData[i]\n        let listItemRolesLength = listItem.roles.length\n        listItem.newMachine = []\n        let flag1 = true\n\n        // 遍历listData[i]中的roles\n        for (let j = 0; j < listItemRolesLength; j++) {\n          let listItemRolesItem = listItem.roles[j]\n          let machineLength = listItemRolesItem.machines.length\n\n          if (machineLength) {\n            // 遍历listData[i]中roles中的machines\n            let flag = true\n            for (let k = 0; k < machineLength; k++) {\n              let machineItem = listItemRolesItem.machines[k]\n              let machineIdArr = machineItem.machine_id.split(',')\n              let machineNameArr = machineItem.machine_name.split(',')\n              let machineStatusArr = machineItem.status.split(',')\n              let machineIdArrLength = machineIdArr.length\n              /*\n              * 将 machines ({machine_id: '1,2,3,4', machine_name: '上海终端机，北京终端机'})中的\n              * 属性machine_id (1,2,3,4) 和属性machine_name ('上海终端机,北京店终端机')\n              * 以逗号拆分，再组成[{machine_id: 1, machine_name: '上海终端机'}, {machine_id: 2, machine_name: '北京终端机'}]\n              * 再将push进去listItemRolesItem.newMachine数组中\n              */\n              for (let l = 0; l < machineIdArrLength; l++) {\n              // listData数组中的某一对象，在这里创建是为了防止对象重复引用\n                let tempListItem = {\n                  index: (this.$route.query.pageSize * this.$route.query.pageNum) + i + 1,\n                  id: listItem.id,\n                  account: listItem.account,\n                  phone: listItem.phone,\n                  username: listItem.username,\n                  storeName: listItemRolesItem.stores_name,\n                  storeId: listItemRolesItem.stores_id,\n                  freeStorage: this.handleMemory(machineIdArr[l]),\n                  machineNumber: listItemRolesItem.frequency,\n                  machineItemCount: machineList[i],\n                  machineId: machineIdArr[l],\n                  machineName: machineNameArr[l],\n                  machineStatus: parseInt(machineStatusArr[l])\n                }\n                // 处理合并门店 ，数量\n                if (flag) {\n                  flag = false\n                  tempListItem.span = machineIdArrLength\n                } else {\n                  tempListItem.span = 0\n                }\n                // 处理合并序号-用户姓名-登录账号-绑定手机号码      以及总数量\n                if (flag1) {\n                  flag1 = false\n                  tempListItem.span1 = machineList[i]\n                } else {\n                  tempListItem.span1 = 0\n                }\n                newListData.push(tempListItem)\n              }\n            }\n          }\n        }\n      }\n      this.listData = newListData\n    },\n    // 处理终端机剩余内存\n    handleMemory (machineId) {\n      let memory = this.memory\n      let len = memory.length\n      for (let i = 0; i < len; i++) {\n        if (memory[i].machine_id === machineId) {\n          let arr = memory[i].memory.split(',')\n          return {\n            top: arr[0],\n            cenBottom: arr[1]\n          }\n        }\n      }\n    },\n    // 处理未分发，列表数据\n    handleListData1 (listData, machineList) {\n      let newListData = []\n      /**\n        * 数据由\n        * [{\"id\":21,\n        *   \"roles\":[\n        *     {\"relations\":[],\n        *     \"stores_id\":\"8\",\n        *     \"stores_name\":\"世界之窗\",\n        *     \"stores_city\":\"纽约\",\n        *     \"frequency\":\"2\",\n        *     \"machines\":[{\"machine_id\":\"020,021\",\"machine_name\":\"世界之窗-1,世界之窗-2\",\"status\":\"1,1\"}]\n        *   }]\n        * }] 变成\n        * [\n        *   {id: 21, storeId: 8, storeName: '世界之窗', storeCity: '纽约', frequency: 2, machineId: 020, machineName: '世界之窗-1', machineStatus: 1},\n        *   {id: 21, storeId: 8, storeName: '世界之窗', storeCity: '纽约', frequency: 2, machineId: 020, machineName: '世界之窗-1', machineStatus: 1}\n        * ]\n        */\n      for (let listItem of listData) {\n        let roles = listItem.roles\n        for (let rolesItem of roles) {\n          let machines = rolesItem.machines\n          for (let machinesItem of machines) {\n            let machinesIdArr = machinesItem.machine_id.split(',')\n            let machinesStatusArr = machinesItem.status.split(',')\n            let machinesNameArr = machinesItem.machine_name.split(',')\n            let machinesIdArrLenght = machinesIdArr.length\n\n            let flag = true\n            for (let i = 0; i < machinesIdArrLenght; i++) {\n              let newListItem = {\n                index: (this.$route.query.pageSize * this.$route.query.pageNum) + i + 1,\n                userId: listItem.id,\n                cityName: rolesItem.stores_city,\n                storeId: rolesItem.stores_id,\n                storeName: rolesItem.stores_name,\n                freeStorage: this.handleMemory(machinesIdArr[i]),\n                frequency: parseInt(rolesItem.frequency),\n                machineId: machinesIdArr[i],\n                machineName: machinesNameArr[i],\n                machineStatus: parseInt(machinesStatusArr[i])\n              }\n              if (flag) {\n                flag = false\n                newListItem.span1 = Number(rolesItem.frequency)\n              } else {\n                newListItem.span1 = 0\n              }\n              newListData.push(newListItem)\n            }\n          }\n        }\n      }\n\n      // 将相同的地址的终端机放在一起\n      let tempListData = {}\n      let cityNameArr = []\n\n      newListData.forEach((item, index) => {\n        let cityIndex = cityNameArr.indexOf(item.cityName) + ''\n\n        // cityNameArr中还没有这个城市\n        if (cityIndex === '-1') {\n          let cityNameArrLength = 0\n\n          cityNameArr.push(item.cityName)\n          cityNameArrLength = cityNameArr.length - 1 + ''\n          tempListData[cityNameArrLength] = []\n          tempListData[cityNameArrLength].push(item)\n        } else {\n          tempListData[cityIndex].push(item)\n        }\n      })\n\n      let finalListData = []\n      for (let key in tempListData) {\n        let tempListDataItemLength = tempListData[key].length\n\n        let flag = true\n        tempListData[key].forEach(item => {\n          if (flag) {\n            flag = false\n            item.span = tempListDataItemLength\n          } else {\n            item.span = 0\n          }\n          item.total = item.span\n        })\n        finalListData = [...finalListData, ...tempListData[key]]\n      }\n      this.newListData = finalListData\n    },\n    setToastData ({message, type, callback}) {\n      this.toastStatus = true\n      this.$set(this.toastData, 'message', message)\n      this.$set(this.toastData, 'type', type)\n      setTimeout(() => {\n        this.toastStatus = false\n        callback && callback()\n      }, 1200)\n    },\n    // 控制table的表格合并\n    objectSpanMethod ({row, column, rowIndex, columnIndex}) {\n      let columnNumber = 5\n      if (columnIndex < 4 || columnIndex === 9) {\n        if (row.span1 > 1) {\n          return {\n            rowspan: row.span1,\n            colspan: 1\n          }\n        } else if (row.span1 === 0) {\n          return {\n            rowspan: 0,\n            colspan: 0\n          }\n        } else {\n          return {\n            rowspan: 1,\n            colspan: 1\n          }\n        }\n      } else if ((columnIndex >= 4 && columnIndex < columnNumber) || columnIndex === 8) {\n        if (row.span > 1) {\n          return {\n            rowspan: row.span,\n            colspan: 1\n          }\n        } else if (row.span === 0) {\n          return {\n            rowspan: 0,\n            colspan: 0\n          }\n        } else {\n          return {\n            rowspan: 1,\n            colspan: 1\n          }\n        }\n      }\n    },\n    // 控制table的表格合并\n    objectSpanMethod1 ({row, column, rowIndex, columnIndex}) {\n      if (columnIndex < 2 || columnIndex === 7) {\n        if (row.span > 1) {\n          return {\n            rowspan: row.span,\n            colspan: 1\n          }\n        } else if (row.span === 0) {\n          return {\n            rowspan: 0,\n            colspan: 0\n          }\n        } else {\n          return {\n            rowspan: 1,\n            colspan: 1\n          }\n        }\n      } else if (columnIndex === 2 || columnIndex === 6) {\n        if (row.span1 > 1) {\n          return {\n            rowspan: row.span1,\n            colspan: 1\n          }\n        } else if (row.span1 === 0) {\n          return {\n            rowspan: 0,\n            colspan: 0\n          }\n        } else {\n          return {\n            rowspan: 1,\n            colspan: 1\n          }\n        }\n      } else {\n        return {\n          rowspan: 1,\n          colspan: 1\n        }\n      }\n    }\n  }\n}\n</script>\n<style lang=\"stylus\">\n  .terminal-table-container .el-table--enable-row-hover .el-table__body tr:hover>td\n    background-color #fff\n</style>\n<style lang=\"stylus\" scoped>\n  .red\n    color #F1524A\n  .list-header\n    padding 16px 40px 16px 20px\n    display flex\n    align-items center\n    border-bottom 1px solid #f5f5f5\n    &.list-header-special\n      padding 30px 40px 0 30px\n  .list-header-button\n    width 120px\n    height 32px\n    line-height 32px\n    text-align center\n    border-radius 4px 4px 0 0\n    color #B1B1B1\n    background-color #EBEBEB\n    cursor pointer\n    &.active\n      border 1px solid #2162D2\n      color #ffffff\n      background-color #2162D2\n  .list-header-select-container\n    margin-right 20px\n  .list-header + .list-header\n    display flex\n    justify-content space-between\n    align-items center\n    .list-header-left\n      display flex\n    .list-header-right\n      display flex\n      align-items center\n      font-size 12px\n      color #999999\n      .online-icon\n        margin-right 5px\n        display inline-block\n        height 7px\n        width 7px\n        border-radius 50%\n        background-color #3ED572\n      .outline-icon\n        margin-right 5px\n        margin-left 30px\n        display inline-block\n        height 7px\n        width 7px\n        border-radius 50%\n        background-color #F1524A\n  .list-title\n    font-size 18px\n  .add-container\n    display flex\n    align-items center\n  .add-icon\n    display inline-block\n    height 38px\n    width 38px\n    background url('~@/assets/set_up.png') no-repeat center\n    background-size contain\n    cursor pointer\n  .add-text\n    margin-left 10px\n    font-size 14px\n    color #666666\n    cursor pointer\n  #list-table\n    margin-top 0\n  .select-auth-td\n    color #a8a8a8\n  .el-checkbox-container\n    display inline-block\n    width 130px\n  