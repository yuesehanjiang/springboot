Rules\" ref=\"oldPassForm\">\n          <el-form-item  style='width: 370px' label=\"原密码:\" prop=\"oldPassword\">\n            <el-input type='password' placeholder=\"密码由 8 - 20 位字母和数字组成\" v-model=\"oldPassForm.oldPassword\"></el-input>\n          </el-form-item>\n        </el-form>\n        <el-form class=\"form\" :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-position=\"left\" label-width=\"120px\">\n          <el-form-item style='width: 370px; position: relative' label=\"验证码:\" prop=\"captcha\">\n            <el-input :maxlength=\"6\" placeholder=\"验证码必须是 6 位数字\" v-model=\"ruleForm.captcha\"></el-input>\n            <span @click=\"sendSms\" :class=\"{active: sendSmsStatus}\" class=\"send-code\">{{sendSmsTips}}</span>\n          </el-form-item>\n          <el-form-item style='width: 370px' label=\"新密码:\" prop=\"newPassword\">\n            <el-input :maxlength=\"20\" type='password' placeholder=\"密码由 8 - 20 位字母和数字组成\" v-model=\"ruleForm.newPassword\"></el-input>\n          </el-form-item>\n          <el-form-item style='width: 370px' label=\"确认新密码:\" prop=\"confirmPassword\">\n            <el-input type='password' :maxlength=\"20\" placeholder=\"密码由 8 - 20 位字母和数字组成\" v-model=\"ruleForm.confirmPassword\"></el-input>\n          </el-form-item>\n        </el-form>\n        <div class=\"form-footer\">\n            <span @click='resetForm' class='cancel-button'>重置</span>\n            <span @click='confirm' class='confirm-button'>确定</span>\n        </div>\n        <toast\n          v-if='toastStatus'\n          :message='toastData.message'\n          :type='toastData.type'>\n        </toast>\n    </div>\n</template>\n\n<script>\nimport { sendUserSms } from 'api/common'\nimport { getToken, logout } from 'utils/tool'\nimport { updateUserPassword } from 'api/user'\nimport { checkPassword, checkCaptcha } from 'utils/formvalidate'\nimport Toast from 'components/Toast'\nexport default {\n  components: {Toast},\n  data () {\n    const validateConfirmPassword = (rule, value, callback) => {\n      if (this.ruleForm.newPassword === this.ruleForm.confirmPassword) {\n        callback()\n      } else {\n        callback(new Error('两次密码不一致'))\n      }\n    }\n    return {\n      oldPassForm: {\n        oldPassword: ''\n      },\n      oldPassRules: {\n        oldPassword: checkPassword()\n      },\n\n      ruleForm: {\n        captcha: '',\n        newPassword: '',\n        confirmPassword: ''\n      },\n      rules: {\n        captcha: checkCaptcha(),\n        newPassword: checkPassword(),\n        confirmPassword: [\n          { required: true, message: '确认密码不能为空', trigger: 'blur' },\n          {required: true, trigger: 'blur', validator: validateConfirmPassword}\n        ]\n      },\n\n      // toast\n      toastStatus: false,\n      toastData: {},\n\n      sendSmsStatus: false,\n      sendSmsTips: '发送验证码',\n      count: 60, // 倒计时60秒\n\n      phone: '',\n      account: ''\n    }\n  },\n  mounted () {\n    let userData = getToken()\n    this.userData = userData\n    this.phone = userData.phone\n    this.account = userData.account\n  },\n  methods: {\n    sendSms () {\n      this.$refs.oldPassForm.validate(valid => {\n        if (!this.sendSmsStatus && valid) {\n          let form = {\n            phone: this.phone\n          }\n          this.cutdownTime()\n          sendUserSms(form).then(res => {\n            if (res.code === '200') {\n              this.setToastData({\n                message: res.message,\n                type: 'success'\n              })\n            } else {\n              this.setToastData({\n                message: res.message,\n                type: 'fail'\n              })\n            }\n          })\n        }\n      })\n    },\n    cancel () {\n      this.myCroppa.remove()\n    },\n    confirm () {\n      this.$refs.oldPassForm.validate(valid1 => {\n        this.$refs.ruleForm.validate(valid => {\n          if (valid && valid1) {\n            let form = {\n              user_id: getToken().id,\n              phone: this.phone,\n              original: this.oldPassForm.oldPassword,\n              code: this.ruleForm.captcha,\n              password: this.ruleForm.newPassword,\n              account: this.userData.account\n            }\n            updateUserPassword(form).then(res => {\n              if (res.code === '200') {\n                this.setToastData({\n                  message: '密码修改成功，需要重新登陆。',\n                  callback () {\n                    logout('user')\n                  }\n                })\n              } else {\n                this.setToastData({\n                  message: res.message,\n                  type: 'fail'\n                })\n              }\n            })\n          }\n        })\n      })\n    },\n    resetForm () {\n      this.$refs.ruleForm.resetFields()\n    },\n    cutdownTime () {\n      this.sendSmsStatus = true\n      this.sendSmsTips = `倒计时${this.count}秒`\n      this.count = this.count - 1\n      this.timer = setTimeout(this.cutdownTime, 1000)\n      if (this.count < 0) {\n        this.sendSmsTips = '发送验证码'\n        this.count = 60\n        this.sendSmsStatus = false\n        clearTimeout(this.timer)\n      }\n    },\n    setToastData ({message, type, callback}) {\n      this.toastStatus = true\n      this.$set(this.toastData, 'message', message)\n      this.$set(this.toastData, 'type', type)\n      setTimeout(() => {\n        this.toastStatus = false\n        callback()\n      }, 1200)\n    }\n  }\n}\n</script>\n\n<style lang=\"stylus\" scoped>\n.form\n  margin-left 20px\n.form-title\n  padding-left 16px\n  margin 30px auto\n  height 23px\n  line-height 23px\n  border-left 4px solid #F1524A\n  font-size 16px\n  color #666666\n  font-weight normal\n.form-item\n  margin 36px 0 36px 20px\n  line-height 16px\n.form-label\n  display inline-block\n  width 118px\n  font-size 14px\n  color #333333\n.form-value\n  font-size 16px\n.send-code\n  position absolute\n  top 0\n  left 260px\n  width 90px\n  height 33px\n  l