AAAE,WAAA,wBAAAhD,EAAAmD,SAA8F,UAAWnD,EAAAU,GAAA,KAAAP,EAAA,mBAAoCG,OAAOkC,KAAA,UAAAjG,MAAA,MAA8BkG,YAAAzC,EAAA0C,KAAsBjB,IAAA,UAAAkB,GAAA,SAAAC,GAAiC,OAAAzC,EAAA,OAAkBE,YAAA,sBAAgCF,EAAA,eAAoBE,YAAA,uBAAAC,OAA0C8C,IAAMvE,KAAA,iBAAA+D,EAAAE,IAAAzE,GAAAnB,OAA8CoB,QAAAsE,EAAAE,IAAAxE,aAAgC6B,EAAA,OAAAH,EAAAU,GAAA,KAAAP,EAAA,UAAqCE,YAAA,yBAAAgB,IAAyCgC,MAAA,SAAAtB,GAAyB/B,EAAA7B,aAAAyE,EAAAE,SAA8B3C,EAAA,mBAAqB,OAAAH,EAAAU,GAAA,KAAAV,EAAAvD,QAA2WuD,EAAAmD,KAA3WhD,EAAA,OAA+CE,YAAA,uBAAiCF,EAAA,QAAaG,OAAOgD,MAAAlE,OAAAY,EAAArD,SAAA2G,OAAAC,QAAAnE,OAAAY,EAAArD,SAAAc,SAAA,EAAA+F,YAAApE,OAAAY,EAAArD,SAAA8C,UAAAgE,aAAA,GAAAC,gBAAA,GAAAC,aAAA,GAAAvC,UAAA,GAA0LC,IAAKC,YAAAtB,EAAAT,cAAAqE,sBAAA5D,EAAAR,mBAAwE,KAAAQ,EAAAU,GAAA,UAAAV,EAAApD,SAAAuD,EAAA,SAAgEG,OAAOuD,WAAA,KAAArH,QAAAwD,EAAAnD,YAA2CwE,IAAKyC,YAAA,SAAA/B,GAA+B/B,EAAApD,UAAA,GAAmBmH,aAAA,SAAAhC,GAAiC/B,EAAAN,eAAAM,EAAApD,cAAmCoD,EAAAU,GAAA,oBAAAV,EAAAmD,MAAA,IAE5iIa,oBCCjB,IAcAC,EAdAzD,EAAA,OAcA0D,CACAlI,EACA8D,GATA,EAVA,SAAAqE,GACA3D,EAAA,SAaA,kBAEA,MAUA4D,EAAA,QAAAH,EAAA","file":"static/js/37.7641f182d8aa46ff3b50.js","sourcesContent":["<template>\n  <div class=\"screen-container\">\n    <!-- <div class=\"screen-empty\" v-if=\"isEmpty\">\n      <img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">\n      <p class=\"empty-title\">没有找到问候语</p>\n    </div> -->\n    <div class=\"screen-empty\" v-if=\"isNoPower\">\n      <img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">\n      <p class=\"empty-title\">没有模块权限，请与管理员确认</p>\n    </div>\n    <div class=\"screen-body\" v-else v-loading=\"loading\">\n      <div class=\"screen-body-header\">\n        <Select size=\"default\" :clearable=\"true\" placeholder=\"门店\" :transfer=\"false\" style=\"width:200px\"\n                :value=\"$route.query.stores_id\" @on-change=\"(value)=>{selected_store=value}\">\n          <Option v-for=\"(item) in stores\" :key=\"item.stores_id\" :value=\"item.stores_id\">{{item.stores_name}}</Option>\n        </Select>\n        <Select size=\"default\" :clearable=\"true\" placeholder=\"问候语状态\" :transfer=\"false\" style=\"width:200px;margin:0 20px\"\n                :value=\"$route.query.isNull\" @on-change=\"(value)=>{selected_status=value}\">\n          <Option value=\"0\">已添加问候语</Option>\n          <Option value=\"1\">未添加问候语</Option>\n        </Select>\n        <Input size=\"default\" prefix=\"ios-search\" placeholder=\"姓名/ID/卡号搜索\" style=\"width: 200px\"\n               :value=\"$route.query.keyword\" @on-keyup.enter=\"(e)=>{keyword=e.target.value}\"/>\n      </div>\n      <div class=\"screen-body-container\">\n        <el-table\n          :header-cell-style=\"{'text-align': 'center'}\"\n          style=\"text-align: center\"\n          height=\"100%\"\n          :data=\"listData.data\"\n          border>\n          <el-table-column\n            prop=\"id\"\n            width=\"70\"\n            label=\"序号\">\n          </el-table-column>\n          <el-table-column\n            prop=\"stores_name\"\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_name\"\n            label=\"会员姓名\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_id\"\n            label=\"会员ID\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_cards\"\n            label=\"卡号\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_pic\"\n            label=\"头像\">\n            <template slot-scope=\"scope\">\n              <div>\n                <table-image :table-image-url='scope.row.user_pic'>\n                  <img class=\"user-avatar\" :src=\"scope.row.user_pic\" alt=\"user-avatar\">\n                </table-image>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"exclusive\"\n            label=\"专属问候语\">\n            <template slot-scope=\"scope\">\n              <p class=\"text-ellipsis\">\n                <template v-for=\"(str,index) in scope.row.exclusive.split(scope.row.user_name)\">\n                  <span :key=\"'str'+index\">{{str}}</span>\n                  <span :key=\"'m'+index\" class=\"greetings-member\"\n                        v-if=\"(index+1)!==scope.row.exclusive.split(scope.row.user_name).length\">\n                    {{scope.row.user_name}}\n                  </span>\n                </template>\n              </p>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"address\"\n            label=\"操作\">\n            <template slot-scope=\"scope\">\n              <div class=\"operate-btn-group\">\n                <router-link :to=\"{ path: 'custom/detail/'+scope.row.id, query: { user_id: scope.row.user_id }}\"\n                             class=\"operate-btn edit-btn\"><i></i></router-link>\n                <button class=\"operate-btn delete-btn\"\n                        @click=\"handleDelete(scope.row)\"><i></i></button>\n              </div>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <div v-if='!isEmpty' class=\"screen-body-footer\">\n        <Page\n          :total=\"Number(listData.total)\"\n          :current=\"Number(listData.pageNum)+1\"\n          :page-size=\"Number(listData.pageSize)\"\n          show-sizer show-elevator show-total :transfer=\"false\"\n          @on-change=\"pageNumChange\"\n          @on-page-size-change=\"pageSizeChange\"\n        />\n      </div>\n    </div>\n    <Modal\n      v-if=\"deleting!==false\"\n      modalTitle=\"删除\"\n      :loading=\"isDeleting\"\n      @cancelModal=\"deleting=false\"\n      @confirmModal=\"deleteGreeting(deleting)\"\n    >确认删除专属问候语吗\n    </Modal>\n  </div>\n</template>\n<script>\nimport { getCustomGreetings, delCustomGreeting } from 'api/greetings'\nimport { getToken } from 'utils/tool'\n\nimport Modal from 'components/Modal'\nimport TableImage from '../components/TableImage'\n\nexport default {\n  components: {Modal, TableImage},\n  data () {\n    return {\n      cityList: [\n        {\n          value: 'New York',\n          label: 'New York'\n        },\n        {\n          value: 'London',\n          label: 'London'\n        },\n        {\n          value: 'Sydney',\n          label: 'Sydney'\n        },\n        {\n          value: 'Ottawa',\n          label: 'Ottawa'\n        },\n        {\n          value: 'Paris',\n          label: 'Paris'\n        },\n        {\n          value: 'Canberra',\n          label: 'Canberra'\n        }\n      ],\n      loading: true,\n      isEmpty: false,\n      isNoPower: false,\n      listData: {},\n      deleting: false,\n      isDeleting: false,\n      stores: [],\n      selected_status: this.$route.query.isNull,\n      selected_store: this.$route.query.stores_id,\n      keyword: ''\n    }\n  },\n  watch: {\n    selected_store: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        stores_id: value === undefined ? '' : String(value)\n      })\n    },\n    selected_status: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        isNull: value === undefined ? '' : String(value)\n      })\n    },\n    keyword: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        keyword: value || ''\n      })\n    },\n    '$route': function () {\n      this.getList(Object.assign({}, this.$route.query))\n    }\n  },\n  mounted: function () {\n    this.token = getToken()\n    this.getList(Object.assign({}, this.$route.query))\n  },\n  methods: {\n    handleDelete (data) {\n      if (data.exclusive === '-/-') {\n        this.$message({\n          message: '无专属问候语',\n          type: 'warning'\n        })\n        return\n      }\n      this.deleting = data.id + ',' + data.user_id\n    },\n    easyQuery (data) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      if (query.isNull === '') {\n        delete query.isNull\