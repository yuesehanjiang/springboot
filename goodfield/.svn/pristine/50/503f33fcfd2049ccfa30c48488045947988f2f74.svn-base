 in item.name.split('{{name}}')\">\n                <span :key=\"'str'+index\">{{str}}</span>\n                <span :key=\"'m'+index\" class=\"greetings-member\"\n                      v-if=\"(index+1)!==item.name.split('{{name}}').length\">会员名称</span>\n              </template>\n            </div>\n          </div>\n          <!--<div class=\"greetings-item\">-->\n          <!--<div class=\"greetings-label\">非会员问候语</div>-->\n          <!--<div class=\"greetings-content\">{{item.nonmemberName}}</div>-->\n          <!--</div>-->\n          <div class=\"greetings-menu\">\n            <p class=\"greetings-menu-title\">通用问候语{{index+1}}</p>\n            <router-link :to=\"'/user/greetings/common/detail/'+item.id\" tag=\"div\" class=\"greetings-edit-btn\">\n              <i class=\"edit-icon\"></i>\n            </router-link>\n            <div class=\"greetings-delete-btn\" @click=\"deleting=item.id\"><i class=\"delete-btn\"></i></div>\n          </div>\n        </div>\n      </div>\n      <div class=\"screen-body-footer\">\n        <Page\n          :total=\"Number(listData.total)\"\n          :current=\"Number(listData.pageNum)+1\"\n          :page-size=\"Number(listData.pageSize)\"\n          show-sizer show-elevator show-total :transfer=\"false\"\n          @on-change=\"pageNumChange\"\n          @on-page-size-change=\"pageSizeChange\"\n        />\n      </div>\n    </div>\n    <Modal\n      v-if=\"deleting!==false\"\n      modalTitle=\"删除\"\n      :loading=\"isDeleting\"\n      @cancelModal=\"deleting=false\"\n      @confirmModal=\"deleteGreeting(deleting)\"\n    >确认删除该通用问候语吗\n    </Modal>\n  </div>\n</template>\n<script>\nimport { getGreetingsInfoList, deleteGreetingsInfo } from 'api/greetings'\nimport { getToken } from 'utils/tool'\nimport Modal from 'components/Modal'\n\nexport default {\n  components: {Modal},\n  data () {\n    return {\n      loading: true,\n      isEmpty: false,\n      isNoPower: false,\n      listData: {},\n      deleting: false,\n      isDeleting: false\n    }\n  },\n  watch: {\n    '$route': function () {\n      this.getList(Object.assign({}, this.$route.query))\n    }\n  },\n  beforeCreate () {\n    let {pageSize, pageNum} = this.$route.query\n    if (typeof pageSize === 'undefined' || typeof pageNum === 'undefined') {\n      this.$router.replace({\n        query: {\n          pageSize: 10,\n          pageNum: 0\n        }\n      })\n    }\n  },\n  mounted: function () {\n    this.token = getToken()\n    this.getList(this.$route.query)\n  },\n  methods: {\n    getList (data) {\n      this.loading = true\n      data['user_id'] = this.token.id\n      getGreetingsInfoList(data).then((res) => {\n        if (Number(res.code) === 200) {\n          this.listData = res\n        } else if (Number(res.code) === 201) {\n          this.isEmpty = true\n        } else if (Number(res.code) === 202) {\n          this.isNoPower = true\n        } else {\n          this.$message({\n            message: res.message,\n            type: 'warning'\n          })\n        }\n        this.loading = false\n      })\n    },\n    pageNumChange (pageNum) {\n      this.easyQuery({\n        pageNum: pageNum - 1\n      })\n    },\n    pageSizeChange (pageSize) {\n      this.easyQuery({\n        pageSize,\n        pageNum: 0\n      })\n    },\n    easyQuery (data) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      this.$router.push({path: this.$route.path, query})\n    },\n    deleteGreeting (id) {\n      this.isDeleting = true\n      deleteGreetingsInfo({id, 'userId': this.token.id}).then(res => {\n        this.isDeleting = false\n        if (Number(res.code) === 200) {\n          this.deleting = false\n          this.getList(this.$route.query)\n          this.$message({\n            message: '删除成功',\n            type: 'success'\n          })\n        } else {\n          this.$message({\n            message: res.message,\n            type: 'warning'\n          })\n        }\n      })\n    }\n  }\n}\n</script>\n<style lang='stylus' scoped>\n  .screen-container\n    flex 1\n    width 100%\n\n  .screen-empty\n    flex 1\n    display flex\n    justify-content center\n    align-items center\n    flex-direction column\n\n  .empty-title\n    font-size 12px\n    color #b1b1b1\n    margin-top 10px\n    > a\n      color #24CF5F\n\n  .screen-body\n    flex 1\n    display flex\n    flex-direction column\n    align-items center\n\n  .screen-body-header\n    height 70px\n    display flex\n    flex-direction row-reverse\n    padding 18px 30px\n    width 100%\n    button\n      border none\n      font-size: 14px;\n      color: #FFFFFF;\n      line-height: 21px;\n      width: 120px;\n      height: 32px;\n      background: rgba(36, 207, 95, 0.9);\n      box-shadow: 0px 0px 2px 0px rgba(36, 207, 95, 1);\n      border-radius: 4px;\n\n  .screen-body-container\n    flex 1\n    width 850px\n    overflow auto\n    padding 10px\n    padding-left 120px\n    box-sizing content-box\n    position relative\n    left -55px\n\n  .screen-body-footer\n    padding-top 30px\n    height 100px\n    width 850px\n    display: flex\n    justify-content flex-end\n\n  .greetings-list\n    position: relative\n    width: 100%;\n    min-height: 192px;\n    background: rgba(255, 255, 255, 1);\n    border-radius: 0px 4px 4px 4px;\n    box-shadow: 0px 0px 6px 0px rgba(102, 102, 102, 0.2);\n    margin-bottom 30px\n    display flex\n    flex-direction column\n    justify-content center\n    background url('~@/assets/color_edge.png') center no-repeat\n    background-position bottom center\n    &:last-child\n      margin-bottom 0\n\n  .greetings-item\n    display flex\n    line-height 40px\n    font-size 14px\n\n  .greetings-label\n    text-align right\n    width 164px\n    color #666666\n\n  .greetings-content\n    flex 1\n    margin-left 60px\n    margin-right 187px\n    border-bottom 1px solid #F4F4F4\n    color #333333\n\n  .greetings-menu\n    position: absolute\n    top: 0\n    right 100%