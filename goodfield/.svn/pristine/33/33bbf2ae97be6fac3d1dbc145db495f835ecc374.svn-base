,OAAAiB,MAAAT,UAAA6D,EAAA,eAAkHG,OAAOqC,kBAAAL,EAAAE,IAAAE,YAAsCvC,EAAA,OAAYE,YAAA,cAAAC,OAAiCsC,IAAAN,EAAAE,IAAAE,SAAAG,IAAA,mBAA8C7C,EAAAY,MAAA,GAAAT,EAAA,QAAAH,EAAAS,GAAA,mBAAmD,OAAAT,EAAAS,GAAA,KAAAT,EAAA7D,SAAAR,MAAAqE,EAAA7D,SAAAR,KAAA+C,OAAA,SAAAsB,EAAAlE,OAAAiB,MAAAT,UAAA6D,EAAA,OAA0HE,YAAA,uBAAiCF,EAAA,QAAaG,OAAOwC,MAAA5D,OAAAc,EAAA7D,SAAA2G,OAAAC,QAAA7D,OAAAc,EAAA7D,SAAAsB,SAAA,EAAAuF,YAAA9D,OAAAc,EAAA7D,SAAAqD,UAAAyD,aAAA,GAAAC,gBAAA,GAAAC,aAAA,GAAAjC,UAAA,GAA0LE,IAAKC,YAAArB,EAAAJ,cAAAwD,sBAAApD,EAAAH,mBAAwE,GAAAG,EAAAY,QAAA,IAE7xGyC,oBCEjB,IAcAC,EAdAC,EAAA,OAcAC,CACAjI,EACAuE,GATA,EAXA,SAAA2D,GACAF,EAAA,QACAA,EAAA,SAaA,kBAEA,MAUAG,EAAA,QAAAJ,EAAA","file":"static/js/4.05b85550a90079e8d599.js","sourcesContent":["<template>\n  <div class=\"screen-container\">\n    <Breadcrumb>\n      <router-link tag=\"li\" :to=\"'/user/report/member'\">入场报告</router-link>\n      <li v-if=\"this.listData.data&&this.listData.data[0]&&this.listData.data[0].user_name\">\n        {{this.listData.data[0].user_name}}\n      </li>\n    </Breadcrumb>\n    <!-- <div class=\"screen-empty\" v-if=\"isEmpty\">\n      <img src=\"~@/assets/empty_logo.gif\" alt=\"empty_logo\">\n      <p class=\"empty-title\">没有更多会员了</p>\n    </div> -->\n    <div class=\"screen-body\" v-loading=\"loading\">\n      <div class=\"screen-body-header\">\n        <DatePicker type=\"daterange\" size=\"default\" :transfer=\"false\" placeholder=\"选择日期\" class=\"query-date\"\n                    :value=\"datetime\" @on-change=\"dateChange\"/>\n        <Select size=\"default\" :clearable=\"true\" placeholder=\"门店\" :transfer=\"false\" class=\"query-select\"\n                style=\"width:200px\"\n                :value=\"$route.query.stores_id\" @on-change=\"(value)=>{stores_id=value}\">\n          <Option v-for=\"(item) in stores\" :key=\"item.stores_id\" :value=\"item.stores_id\">{{item.stores_name}}</Option>\n        </Select>\n        <a v-if=\"listData.data&&listData.data.length\" class=\"export-table\" :href=\"exportHref\">导出表格</a>\n        <!--<Select size=\"default\" :clearable=\"true\" placeholder=\"会员\" :transfer=\"false\" class=\"query-select\"-->\n        <!--style=\"width:200px\"-->\n        <!--:value=\"$route.query.user_type\" @on-change=\"(value)=>{user_type=value}\">-->\n        <!--<Option value=\"0\">曾会员</Option>-->\n        <!--<Option value=\"1\">会员</Option>-->\n        <!--<Option value=\"2\">非会员</Option>-->\n        <!--</Select>-->\n      </div>\n      <div class=\"screen-body-container\">\n        <el-table\n          class=\"list\"\n          :header-cell-style=\"{'text-align': 'center'}\"\n          style=\"text-align: center\"\n          height=\"100%\"\n          :data=\"listData.data\"\n          border>\n          <el-table-column\n            v-if='$route.query.user_type == 2'\n            width=\"70\"\n            label=\"序号\">\n            <template slot-scope=\"scope\">\n              <span>\n                1\n              </span>\n            </template>\n          </el-table-column>\n          <el-table-column\n            v-else\n            prop=\"index\"\n            width=\"70\"\n            label=\"序号\">\n          </el-table-column>\n          <el-table-column\n            v-if=\"type==='store'\"\n            prop=\"stores_name\"\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_name\"\n            label=\"会员姓名\">\n          </el-table-column>\n          <el-table-column\n            v-if=\"type==='member'\"\n            prop=\"stores_name\"\n            label=\"门店\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_id\"\n            label=\"会员ID\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_card\"\n            label=\"卡号\">\n          </el-table-column>\n          <el-table-column\n            prop=\"user_in_time\"\n            label=\"入场时间\">\n            <template slot-scope=\"scope\">\n              <div>{{scope.row.user_in_time}}</div>\n            </template>\n          </el-table-column>\n          <el-table-column\n            prop=\"exclusive\"\n            label=\"图像\">\n            <template slot-scope=\"scope\">\n              <div>\n                <div v-if=\"scope.row.user_pic !== '-/-'\">\n                  <table-image v-if=\"$route.query.user_type !== '2'\" :table-image-url='scope.row.user_pic'>\n                    <img class=\"user-avatar\" :src=\"scope.row.user_pic\" alt=\"user-avatar\">\n                  </table-image>\n                </div>\n                <span v-else>-/-</span>\n              </div>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <div v-if=\"listData.data && listData.data.length > 0 && $route.query.user_type !== '2'\"\n           class=\"screen-body-footer\">\n        <Page\n          :total=\"Number(listData.total)\"\n          :current=\"Number(listData.pageNum)+1\"\n          :page-size=\"Number(listData.pageSize)\"\n          show-sizer show-elevator show-total :transfer=\"false\"\n          @on-change=\"pageNumChange\"\n          @on-page-size-change=\"pageSizeChange\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport Breadcrumb from 'views/user/components/Breadcrumb'\nimport { getToken } from 'utils/tool'\nimport { getMemberReportDetail, getStoreReportDetail } from 'api/report'\nimport Utils from 'utils/utils'\nimport TableImage from '../components/TableImage'\n\nexport default {\n  components: {Breadcrumb, TableImage},\n  data () {\n    let type = 'member'\n    if (this.$route.path.indexOf('/user/report/store/') === 0) {\n      type = 'store'\n    }\n    return {\n      loading: true,\n      isEmpty: false,\n      listData: {},\n      stores: [],\n      stores_id: '',\n      user_type: '',\n      datetime: [],\n      type,\n      token: {}\n    }\n  },\n  computed: {\n    exportHref () {\n      let data = {\n        user_id: this.token.id,\n        u_id: this.$route.params.id\n      }\n      let query = Object.assign({}, data, this.$route.query)\n      delete query['pageNum']\n      delete query['pageSize']\n      return '/goodfield/web/entrancereport/store/exportDetails?' +\n        Object.keys(query).map(v => v + '=' + (query[v] || '')).join('&')\n    }\n  },\n  watch: {\n    stores_id: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        stores_id: value === undefined ? '' : String(value)\n      })\n    },\n    user_type: function (value) {\n      this.easyQuery({\n        pageNum: 0,\n        user_type: value === undefined ? '1' : String(value)\n      })\n    },\n    '$route': function (value) {\n      let {query} = value\n      this.getList(Object.assign({}, query))\n      if (query.datetime) {\n        this.datetime = [query.datetime, query.datetime]\n      } else if (query.startTime && query.endTime) {\n        this.datetime = [query.startTime, query.endTime]\n      }\n    }\n  },\n  created () {\n    this.easyQuery({\n      pageNum: 0,\n      datetime: '',\n      startTime: '',\n      endTime: ''\n    })\n  },\n  mounted () {\n    this.token = getToken()\n    this.getList(Object.assign({}, this.$route.query))\n  },\n  methods: {\n    dateChange (date) {\n      if (date[0] === date[1]) {\n        this.easyQuery({\n          pageNum: 0,\n          datetime: date[0],\n          startTime: '',\n          endTime: ''\n        })\n      } else {\n        this.easyQuery({\n          pageNum: 0,\n          datetime: '',\n          startTime: date[0],\n          endTime: date[1]\n        })\n      }\n    },\n    easyQuery (data) {\n      let prevQuery = this.$route.query\n      let query = Object.assign({}, prevQuery, data)\n      this.$router.push({path: this.$route.path, query})\n    },\n    getList (data = {}) {\n      this.loading = true\n      data['u_id'] = this.$route.params.id\n      data['user_id'] = this.token.id\n      data['pageNum'] = data['pageNum'] || 0\n      dat